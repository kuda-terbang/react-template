{
  "root": "packages/design-token",
  "sourceRoot": "packages/design-token/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/design-token",
        "main": "packages/design-token/src/index.ts",
        "tsConfig": "packages/design-token/tsconfig.lib.json",
        "assets": [
          "packages/design-token/*.md",
          {
            "input": "./packages/design-token/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/design-token/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/design-token",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/design-token",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/design-token/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/design-token"],
      "options": {
        "jestConfig": "packages/design-token/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
