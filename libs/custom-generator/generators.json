{
  "$schema": "http://json-schema.org/schema",
  "name": "custom-generator",
  "version": "0.0.1",
  "generators": {
    "design-token-template": {
      "factory": "./src/generators/design-token-template/design-token-template",
      "schema": "./src/generators/design-token-template/schema.json",
      "description": "design-token-template generator"
    },
    "icons-template": {
      "factory": "./src/generators/icons-template/icons-template",
      "schema": "./src/generators/icons-template/schema.json",
      "description": "generate icons template"
    },
    "react-dashboard-template": {
      "factory": "./src/generators/react-dashboard-template/react-dashboard-template",
      "schema": "./src/generators/react-dashboard-template/schema.json",
      "description": "generate react dashboard"
    },
    "ui-mui-react-component": {
      "factory": "./src/generators/ui-mui-react-template/ui-mui-react-template",
      "schema": "./src/generators/ui-mui-react-template/schema.json",
      "description": "Generate shared component library made with material ui react"
    }
  }
}
