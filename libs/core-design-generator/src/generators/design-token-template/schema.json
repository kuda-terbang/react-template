{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "CustomGenerator",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "tags": {
      "type": "string",
      "description": "Add tags to the project (used for linting)",
      "alias": "t"
    },
		"version": {
      "type": "string",
      "description": "Add version as starter version",
			"default": "0.1.0",
      "alias": "v"
    }
  },
  "required": ["name"]
}
